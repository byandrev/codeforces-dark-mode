{
  "manifest_version": 3,
  "name": "Codeforces Dark Mode",
  "version": "0.1.0",
  "description": "Dark mode for Codeforces",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
